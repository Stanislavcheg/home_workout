%html
  %head
    %meta{content: "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Home workout
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'

  %body
    .using-border.mb-4
      %nav{'aria-label' => 'breadcrumb'}
        %ol.breadcrumb
          = render_breadcrumbs tag: :li, separator: ''

    = yield
s
