#gymnastic_rings
  .container.py-lg-5.py-md-4.py-sm-4.py-3
    .row.agile-service-grid.pt-lg-4.pt-md-4.pt-3
      - @days.each do |day|
        .col-lg-4.col-sm-12.text-center.gap-1
          .white-shadow
            = link_to inline_svg_tag('calendar.svg'), gymnastic_rings_program_day_path(program_id: @program.id, id: day.id)
            .ser-inner-info
              %h4
                = day.name
